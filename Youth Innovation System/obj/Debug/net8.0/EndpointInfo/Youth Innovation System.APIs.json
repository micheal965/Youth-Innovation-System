        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "ConfirmPassword",
                  "Email",
                  "FirstName",
                  "LastName",
                  "Password",
                  "PhoneNumber"
                ],
                "type": "object",
                "properties": {
                  "Email": {
                    "type": "string",
                    "format": "email"
                  },
                  "Password": {
                    "type": "string",
                    "format": "password"
                  },
                  "ConfirmPassword": {
                    "type": "string",
                    "format": "password"
                  },
                  "FirstName": {
                    "maxLength": 50,
                    "minLength": 0,
                    "type": "string"
                  },
                  "LastName": {
                    "maxLength": 50,
                    "minLength": 0,
                    "type": "string"
                  },
                  "PhoneNumber": {
                    "type": "string",
                    "format": "tel"
                  },
                  "ProfilePicture": {
                    "type": "string",
                    "format": "binary"
                  },
                  "role": {
                    "type": "integer",
                    "format": "int32"
                  }
                }
              },
              "encoding": {
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "ConfirmPassword": {
                  "style": "form"
                },
                "FirstName": {
                  "style": "form"
                },
                "LastName": {
                  "style": "form"
                },
                "PhoneNumber": {
                  "style": "form"
                },
                "ProfilePicture": {
                  "style": "form"
                },
                "role": {
                  "style": "form"
                }
              }
